{
  "name": "QuestionnaireGame",
  "plural": "QuestionnaireGames",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "start_date": {
      "type": "date",
      "required": false
    },
    "finish_date": {
      "type": "date",
      "required": false
    },
    "question_time": {
      "type": "number",
      "required": false
    },
    "questionnaire_time": {
      "type": "number",
      "required": false
    },
    "results": {
      "type": {},
      "required": false
    },
    "points": {
      "type": "array",
      "required": false
    },
    "prizes": {
      "type": "array",
      "required": false
    }
  },
  "validations": [],
  "relations": {
    "students": {
      "type": "hasAndBelongsToMany",
      "model": "Student",
      "foreignKey": "studentId"
    },
    "teacher": {
      "type": "belongsTo",
      "model": "Teacher",
      "foreignKey": "teacherId"
    },
    "questionnaire": {
      "type": "hasAndBelongsToMany",
      "model": "Questionnaire",
      "foreignKey": "questionnaireId"
    }
  },
  "acls": [{
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
